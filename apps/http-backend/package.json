    "@types/cors": "^2.8.17",
    "@types/dotenv": "^8.2.3",
    "@types/express": "^5.0.1",
    "@types/jsonwebtoken": "^9.0.9",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "esbuild": "^0.25.2",
    "esbuild-runner": "^2.2.2",
    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.2"
  },
